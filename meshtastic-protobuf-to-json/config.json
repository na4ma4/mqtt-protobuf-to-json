{
  "name": "Meshtastic Protobuf to JSON",
  "version": "latest",
  "slug": "meshtastic_protobuf_to_json",
  "description": "Convert Meshtastic protobuf messages to JSON and publish them back to MQTT",
  "url": "https://github.com/na4ma4/mqtt-protobuf-to-json/tree/master/meshtastic-protobuf-to-json",
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "hassio_api": true,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "boot": "auto",
  "init": false,
  "ingress": true,
  "timeout": 30,
  "panel_icon": "mdi:bigtop_updates",
  "map": [
    {
      "type": "share",
      "read_only": false
    },
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/config"
    },
    {
      "type": "addon_config",
      "read_only": false,
      "path": "/addon_config"
    }
  ],
  "options": {
    "data_path": "/config/meshtastic_protobuf_to_json"
  },
  "schema": {
    "data_path": "str"
  },
  "image": "ghcr.io/na4ma4/mqtt-protobuf-to-json:latest"
}